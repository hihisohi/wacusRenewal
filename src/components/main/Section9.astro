<script is:inline charset="UTF-8">
   //section9
   $(".map_remote li").click(function () {
       $(".map_remote li").removeClass("on");
       $(this).addClass("on");
       var idx = $(this).index();
       $(".building > div").removeClass("on").eq(idx).addClass("on");
       $(".map_group > div").removeClass("on").eq(idx).addClass("on");
   })

   $(window).resize(function(){
       mapHeight();
   });

   $(document).ready(function(){
       mapHeight();
       mapReload();
   });

   function mapHeight(){
       var conNum0;
       var conNum1;
       $('.mainpage .s9_con .map_remote .con').each(function(index){
           if(index == 0) {
               conNum0 = $(this).innerHeight();
           } else if(index == 1) {
               conNum1 = $(this).innerHeight();
           };
           return true;
       });
       if(conNum0 >= conNum1) {
           $('.mainpage .s9_con .map_remote').attr('style','--cH:'+conNum0+'px')
       } else {
           $('.mainpage .s9_con .map_remote').attr('style','--cH:'+conNum1+'px')
       }
   }

   function mapReload(){
           $('.root_daum_roughmap_landing .wrap_map, .root_daum_roughmap_landing .wrap_controllers').remove();
           map1 = new daum.roughmap.Lander({
               "timestamp" : "1730358227556",
               "key" : "2m49v",
               "mapWidth" : "890",
               "mapHeight" : "360",
           }).render();
           map2 = new daum.roughmap.Lander({
               "timestamp" : "1730360357348",
               "key" : "2m4am",
               "mapWidth" : "890",
               "mapHeight" : "360",
           }).render();
       }

</script>


<script is:inline>
    document.querySelector('.send').addEventListener('click', function () {
        // 선택된 항목들 가져오기
        const selectGroups = document.querySelectorAll('.s9_select');
        const inputFields = document.querySelectorAll('#name, #phone, #company, #email, #inquiry');
        let isValid = true; // 선택 여부 확인용
        let formData = {}; // 최종 데이터 저장

        // 각 선택 그룹 확인
        selectGroups.forEach((group, index) => {
            const isMultiSelect = group.classList.contains('s9_select1'); // 다중 선택 여부 확인
            const selectedItems = group.querySelectorAll('li.selected'); // 선택된 항목들

            if (isMultiSelect) {
                // 다중 선택 처리
                if (selectedItems.length > 0) {
                    formData[`select${index + 1}`] = Array.from(selectedItems).map((item) => item.textContent);
                } else {
                    isValid = false;
                    alert(`선택 ${index + 1} 항목을 최소 하나 이상 선택해주세요!`);
                }
            } else {
                // 단일 선택 처리
                const selectedItem = group.querySelector('li.selected');
                if (selectedItem) {
                    formData[`select${index + 1}`] = selectedItem.textContent;
                } else {
                    isValid = false;
                    alert(`선택 ${index + 1} 항목을 선택해주세요!`);
                }
            }
        });

        // 입력 필드 값 저장
        inputFields.forEach((input) => {
            const value = input.value.trim();
            const id = input.getAttribute('id');
            if (!value && id !== 'inquiry') { // 필수 입력 필드 확인
                isValid = false;
                alert(`${input.nextElementSibling.textContent}를 입력해주세요!`);
                return;
            }
            formData[id] = value;
        });

        // 모든 항목이 유효한 경우 처리
        if (isValid) {
            console.log('선택된 데이터:', formData);

            // jQuery AJAX 요청
            $.ajax({
                url: 'https://wacus.co.kr/ajax/bringWacusContactDB.php', // 서버의 API 엔드포인트
                method: 'POST',
                contentType: 'application/json',
                data: JSON.stringify(formData),
                success: function (response) {
                    // 성공 처리
                    alert('문의가 성공적으로 전송되었습니다!');
                    // console.log('서버 응답:', response);
                    window.location.reload();
                },
                error: function (xhr, status, error) {
                    // 에러 처리
                    alert('전송 중 오류가 발생했습니다. 다시 시도해주세요.');
                    // console.error('오류:', error);
                }
            });
        }
    });

    // 리스트 항목 클릭 이벤트
    document.querySelectorAll('.s9_select li').forEach((item) => {
        item.addEventListener('click', function () {
            const parent = this.parentElement;
            if (parent.classList.contains('s9_select1')) {
                // 다중 선택인 경우
                this.classList.toggle('selected');
            } else {
                // 단일 선택인 경우
                parent.querySelectorAll('li').forEach((li) => li.classList.remove('selected'));
                this.classList.add('selected');
            }
        });
    });
</script>



